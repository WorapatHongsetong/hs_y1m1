    "def Int_a_(a, b):\n",
    "    _a_ = list()\n",
    "    for i in range(a):\n",
    "        _a_.append(0)\n",
    "\n",
    "\n",
    "    _a__ = list()\n",
    "    for i in range(b):\n",
    "        _a__.append(1)\n",
    "\n",
    "    p = _a_ \n",
    "    for i in range(len(_a__)):\n",
    "        p.append(_a__[i])\n",
    "    \n",
    "    return float(len(p))\n",
    "\n",
    "print(Int_a_(15,2))"
